### PROJECT SETTINGS

PROJECT_NAME=my_drupal_project
PROJECT_BASE_URL=drupal.docker.localhost
PROJECT_DOCROOT=../

### --- DOCKER-COMPOSE SETTINGS ----

COMPOSE_PROJECT_NAME=drupal

### --- DRUPAL SETTINGS ----

DRUPAL_ENV=local

### --- DATABASE SETTINGS ----

DB_NAME=drupal
DB_USER=drupal
DB_PASSWORD=drupal
DB_ROOT_PASSWORD=password
DB_HOST=mariadb
DB_PORT=3306
DB_DRIVER=mysql

